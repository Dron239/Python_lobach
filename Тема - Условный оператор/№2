a=int(input())
b=int(input())

if a % 2 == 0 and b % 2 == 0:
    print("Произведение чисел, если оба числа четные=", a * b)
if a % 2 != 0 and b % 2 != 0:
    print("Сумма чисел, если оба числа нечетные=", a + b)
if a % 2 == 0 and b % 2 != 0:
    if a>b:
        print("Первое число больше=", a)
    else:
        print("Второе число больше=", b)
if a % 2 != 0 and b % 2 == 0:
    print("Деление чисел=", a / b)
