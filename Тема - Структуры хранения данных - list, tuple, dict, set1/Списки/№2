# подсчитать сколько раз встречается каждый элемент
#Это сделали на лекции - сам бы не додумался


list_1=[10,20,10,20,30,10,30,40]

#10 - 3, 20 - 2, 30 - 2
list_2=[] 
for element in list_1:
    if not element in list_2:
        list_2.append(element)
print("Список неповторяющихся чисел ",list_2)

for element in list_2:
    print(element,"=",list_1.count(element) )
